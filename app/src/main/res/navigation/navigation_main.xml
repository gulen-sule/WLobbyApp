<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/searchResultsFragment">


    <fragment
        android:id="@+id/searchResultsFragment"
        android:name="com.example.wlobbyapp.main.fragments.SearchResultsFragment"
        android:label="SearchResultsFragment" >
        <action
            android:id="@+id/action_searchResultsFragment_to_movieDetailedFragment"
            app:destination="@id/movieDetailedFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieDetailedFragment"
        android:name="com.example.wlobbyapp.main.fragments.MovieDetailedFragment"
        android:label="MovieDetailedFragment" >
        <argument
            android:name="resultData"
            app:argType="com.example.wlobbyapp.data.search.multiSearch.Results"
            app:nullable="true" />
        <action
            android:id="@+id/action_movieDetailedFragment_to_chooseDateFragment"
            app:destination="@id/chooseDateFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseDateFragment"
        android:name="com.example.wlobbyapp.main.fragments.ChooseDateFragment"
        android:label="ChooseDateFragment" >
        <argument
            android:name="movieData"
            app:argType="com.example.wlobbyapp.data.search.multiSearch.Results"
            app:nullable="true" />
        <action
            android:id="@+id/action_chooseDateFragment_to_movieDetailedFragment"
            app:destination="@id/movieDetailedFragment" />
    </fragment>


</navigation>