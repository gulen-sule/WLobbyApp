<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/searchResultsFragment">
    <fragment
        android:id="@+id/searchResultsFragment"
        android:name="com.example.wlobbyapp.view.fragments.SearchResultsFragment"
        android:label="SearchResultsFragment" >
        <action
            android:id="@+id/action_searchResultsFragment_to_movieDetailedFragment"
            app:destination="@id/movieDetailedFragment" />
        <action
            android:id="@+id/action_searchResultsFragment_to_tvDetailedFragment"
            app:destination="@id/tvDetailedFragment" />
        <action
            android:id="@+id/action_searchResultsFragment_to_chooseDateFragment"
            app:destination="@id/chooseDateFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieDetailedFragment"
        android:name="com.example.wlobbyapp.view.fragments.detailedFragments.MovieDetailedFragment"
        android:label="MovieDetailedFragment" >
        <argument
            android:name="movie_id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/tvDetailedFragment"
        android:name="com.example.wlobbyapp.view.fragments.detailedFragments.TvDetailedFragment"
        android:label="TvDetailedFragment" >
        <argument
            android:name="tv_id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/chooseDateFragment"
        android:name="com.example.wlobbyapp.view.fragments.ChooseDateFragment"
        android:label="ChooseDateFragment" >
        <argument
            android:name="resultData"
            app:argType="com.example.wlobbyapp.model.search.multiSearch.MultiSearchResult" />
    </fragment>
</navigation>